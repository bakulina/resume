{{!-- 
Це код container файлу для summary сторінки, 
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
--}}
<div>
 <ul>
    <h1>{{name}}</h1>
    <h5>
    <li> {{birthdate}}</li>
    <li> {{birthplace}}</li>
    <li> {{ deathdate}}</li>
    <li> {{deathplace}}</li>
    <li> {{nationality}}</li>
    <li> {{occupation}}</li>     
 </h5>
  </ul>
 <ul>
 {{#each known_for}}
 <p>{{this}}</p>
   {{/each}} 
   {{#each education }}

   {{ degree}}
   <br>
    {{field}}
   <br>
   {{institution}}
 <br>
   {{year}}
   {{/each}}
  </ul>
  <ul>
  {{#each notable_publications}}
  {{title}}
  <br>
  {{year}}
  <br>
  {{  publisher}}
  {{/each}}
  </ul> 
  <ul>
  {{#each partners}}
  {{name}}
  <br>
  {{ relationship}}
  <br>
  {{years}}
 {{/each}}
 </ul>
 <ul>
    {{#each awards }}
    {{title}}
    <br>
    {{years}}
    <br>
    {{ description}}
    {{/each}}
 </ul>
 <ul>
    {{#each influences }}
    <p>{{this}}</p>
    {{/each}}
 </ul>
 <ul>
    {{#each influenced}}
    <p>{{this}}</p>
    {{/each}}
 </ul>
 <ul>
    {{#each quotes }}
    <p>{{this}}</p>
    {{/each}}
 </ul>
 <ul>
    {{#each major_discoveries}} 
    {{title}}
    <br>
    {{years}}
    <br>
    {{ description}}
    {{/each}}
 </ul>
 <ul>
    {{#with contributions}}
    {{title}}
    <br>
    {{ description}}
    <br>
    {{years}}
    <br>
    {{field}}
    {{/with}}

 </ul>


</div>


 
       

